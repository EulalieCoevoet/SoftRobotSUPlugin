# CMake package configuration file for the plugin @PROJECT_NAME@

@PACKAGE_GUARD@
@PACKAGE_INIT@

set(SOFTROBOTSSU_HAVE_SOFAGUI @SOFTROBOTSSU_HAVE_SOFAGUI@)
set(SOFTROBOTSSU_HAVE_QT5CORE @SOFTROBOTSSU_HAVE_QT5CORE@)

find_package(SofaBase QUIET REQUIRED)

if(SOFTROBOTSSU_HAVE_SOFAGUI)
    find_package(SofaGui QUIET REQUIRED)
endif()
if(SOFTROBOTSSU_HAVE_QT5CORE)
    find_package(Qt5 COMPONENTS Core QUIET REQUIRED)
endif()

if(NOT TARGET @PROJECT_NAME@)
    include("${CMAKE_CURRENT_LIST_DIR}/@PROJECT_NAME@Targets.cmake")
endif()

check_required_components(@PROJECT_NAME@)
